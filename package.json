{
  "name": "sbgck-fsme-test",
  "version": "1.0.0",
  "description": "",
  "bin": {
    "sbgck": "launcher.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/game.ts",
    "startMain": "ts-node src/main.ts",
    "prepublishOnly": "tsc",
    "prepare": "tsc",
    "po": "mkdirp po && mkdirp dist && ttag init en po/en.po && ttag update po/en.po src/game.ts",
    "po2": "rm -f uk.po* && ttag init uk uk.po && ttag update uk.po src/game.ts && ttag po2json uk.po > uk.po.json && cat uk.po.json | jq '.translations.\"\" | keys | .[]' -r | sed '/^$/d' >texts.txt && rm -f uk.po*"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@gerhobbelt/nomnom": "^1.8.4-31",
    "hasbin": "^1.2.3",
    "javascript-state-machine": "^3.1.0",
    "ttag": "^1.7.24"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "csvsync": "^1.3.2",
    "export-to-csv": "^0.2.1",
    "fast-xml-parser": "^3.19.0",
    "node-fetch": "^2.6.1",
    "pofile": "^1.1.1",
    "string-strip-html": "^8.2.13",
    "ts-node": "^9.1.1",
    "ttag-cli": "^1.9.2",
    "typescript": "^4.2.2"
  },
  "babel": {
    "plugins": [
      [
        "@babel/plugin-proposal-private-methods",
        {
          "loose": true
        }
      ]
    ]
  }
}
